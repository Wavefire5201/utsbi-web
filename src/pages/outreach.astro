---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Outreach">
  <section
    class="px-6 py-10 pt-36 md:px-32 2xl:px-80 relative font-OldStandardTT"
  >
    <div class="text-4xl">Outreach</div>
    <div class="text-xl pt-10">
      Explore our growing network of high schools and colleges, showcasing our
      current connections and future outreach goals to expand sustainable
      impact.
    </div>
  </section>
  <section
    class="px-6 py-10 pt-36 md:px-32 2xl:px-80 relative font-OldStandardTT"
  >
    <div id="map" class="map" style="height: 500px;"></div>
  </section>
</Layout>
