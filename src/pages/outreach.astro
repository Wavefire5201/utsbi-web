---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Outreach">
  <section
    class="px-6 py-10 pt-36 md:px-32 2xl:px-80 relative font-OldStandardTT"
  >
    <div class="text-4xl">Outreach</div>
    <div class="text-xl pt-10">
      Explore our growing network of high schools and colleges, showcasing our
      current connections and future outreach goals to expand sustainable
      impact.
    </div>
  </section>
  <section class="px-6 pb-10 md:px-32 2xl:px-80 relative font-OldStandardTT">
    <div class="relative" style="padding-top: 56.25%;">
      <!-- 16:9 Aspect Ratio -->
      <iframe
        src="https://www.google.com/maps/d/embed?mid=1ItBSYv-_H_PJBScpnU8gLMzr9F4qA6Q&ehbc=2E312F"
        class="absolute top-0 left-0 w-full h-full"
        allowfullscreen></iframe>
    </div>
  </section>
</Layout>
